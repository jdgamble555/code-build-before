{
	"name": "code-build",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"prepare": "npm run vite-workaround && npm run smui-theme-light && npm run smui-theme-dark",
		"vite-workaround": "node _helper/vite-workaround.js",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark"
	},
	"devDependencies": {
		"@playwright/test": "^1.25.0",
		"@smui-extra/accordion": "^7.0.0-beta.3",
		"@smui-extra/autocomplete": "^7.0.0-beta.3",
		"@smui/button": "^7.0.0-beta.3",
		"@smui/card": "^7.0.0-beta.3",
		"@smui/checkbox": "^7.0.0-beta.3",
		"@smui/chips": "^7.0.0-beta.3",
		"@smui/circular-progress": "^7.0.0-beta.3",
		"@smui/dialog": "^7.0.0-beta.3",
		"@smui/form-field": "^7.0.0-beta.3",
		"@smui/list": "^7.0.0-beta.3",
		"@smui/menu": "^7.0.0-beta.3",
		"@smui/snackbar": "^7.0.0-beta.3",
		"@smui/tab": "^7.0.0-beta.3",
		"@smui/tab-bar": "^7.0.0-beta.3",
		"@smui/textfield": "^7.0.0-beta.3",
		"@smui/top-app-bar": "^7.0.0-beta.3",
		"@sveltejs/adapter-vercel": "^2.4.1",
		"@sveltejs/kit": "^1.15.5",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"sass": "^1.55.0",
		"smui-theme": "^7.0.0-beta.3",
		"supabase": "^1.38.7",
		"svelte": "^3.58.0",
		"svelte-check": "^2.7.1",
		"svelte-copy": "^1.3.0",
		"svelte-forms-lib": "^2.0.1",
		"svelte-highlight": "^6.2.1",
		"svelte-meta-tags": "^2.7.0",
		"svelte-paginate": "^0.1.0",
		"svelte-time": "^0.7.1",
		"tslib": "^2.5.0",
		"typescript": "^5.0.0",
		"vite": "^4.2.1",
		"vitest": "^0.25.8"
	},
	"type": "module",
	"dependencies": {
		"@github/markdown-toolbar-element": "^2.1.1",
		"@supabase/supabase-js": "^2.20.0",
		"highlight.js": "^11.7.0",
		"j-supabase": "^1.0.16",
		"svelte-forms": "^2.3.1",
		"svelte-markdown": "^0.2.3"
	}
}
